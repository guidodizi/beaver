{
  "extends": ["prettier", "airbnb-base"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "function-paren-newline": "off",
    "no-console": "off",
    "implicit-arrow-linebreak": "off",
    "no-underscore-dangle": "off",
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }]
  },
  "overrides": [
    {
      "files": ["*.spec.js"],
      "rules": {
        "no-undef": "off"
      }
    }
  ]
}
